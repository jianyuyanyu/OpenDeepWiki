{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    "Type": "sqlite"
  },
  "ConnectionStrings": {
    "Default": "Data Source=opendeepwiki.db"
  },
  "Jwt": {
    "SecretKey": "OpenDeepWiki-Default-Secret-Key-Please-Change-In-Production-Environment-2024",
    "Issuer": "OpenDeepWiki",
    "Audience": "OpenDeepWiki",
    "ExpirationMinutes": 1440
  },
  "AI": {
    "Endpoint": "https://api.routin.ai/v1",
    "ApiKey": "",
    "RequestType": "OpenAI"
  },
  "RepositoryAnalyzer": {
    "CleanupAfterProcessing": false,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000
  },
  "WikiGenerator": {
    "CatalogModel": "glm-4.7",
    "ContentModel": "glm-4.7",
    "PromptsDirectory": "prompts",
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000
  },
  "AllowedHosts": "*"
}
